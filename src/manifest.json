{
    "name": "Password Manager Extension",
    "version": "1.0",
    "description": "Extension with Angular",
    "manifest_version": 2,
    "permissions": [
        "tabs",
        "activeTab",
        "webNavigation",
        "*://*/*",
        "https://www.google.com.tr/*"
    ],
    "background": {
        "scripts": [
            "background.js",
            "runtime.js"
        ]
    },
    "page_action": {
        "default_popup": "index.html"
    }
}
